{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "v1.2/signins/{signinId}",
      "methods": [
        "get"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "signinDocument",
      "direction": "in",
      "type": "cosmosDB",
      "databaseName": "v1.2",
      "collectionName": "signins",
      "connectionStringSetting": "thedeskapp_DOCUMENTDB",
      "partitionKey": "{signinId}",
      "sqlQuery": "SELECT * FROM c WHERE c.id={signinId}"
    }
  ]
}