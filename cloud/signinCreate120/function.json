{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "v1.2/signins",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "newSigninDocument",
      "direction": "out",
      "type": "cosmosDB",
      "databaseName": "v1.2",
      "collectionName": "signins",
      "connectionStringSetting": "thedeskapp_DOCUMENTDB"
    }
  ]
}